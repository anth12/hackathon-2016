<table class="highscores" ng-controller="HighscoreController">
    <thead>
        <tr>
            <th>High Scores</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="highscore in model">
            <td class="place">{{ $index+1}}st</td>
            <td class="user">{{ highscore.user.Name }}</td>
            <td class="score">{{ highscore.userGame.Name }}</td>
        </tr>
    </tbody>
</table>