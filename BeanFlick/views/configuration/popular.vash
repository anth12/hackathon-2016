<div class="row" ng-controller="PopularController">
    <div class="carousel">
        <div class="character-1 column-2" ng-repeat="game in popularGames">
            <img src="{{ game.PreviewImage }}" alt="{{ game.Name }}" />
            <div class="overlay">
                {{ game.Name }}
            </div>
        </div>
    </div>
</div>